\=u.instance({instanceName: 'u20_lut',  moduleName: 'ncore3_addr_mod6_lut3',  ports: {in0: 'sum10', in1: 'sum11', out: 'sum20'} })=\
assign sum21 = sum12;

\=u.instance({instanceName: 'u30_lut',  moduleName: 'ncore3_addr_mod6_lut3',  ports: {in0: 'sum20', in1: 'sum21', out: 'out'} })=\

assign result = out;

endmodule 
