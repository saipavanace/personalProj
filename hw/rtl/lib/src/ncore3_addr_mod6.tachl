\jsbegin
//=============================================================================
// Filename: ncore3_addr_mod6.tachl
// Author: Boon Chuan
// Description: Ncore3 Address Modulo 6
//=============================================================================

var u = obj.lib;

u.paramDefault('wOffset', 'int', 6); // 2 ^ (granularity block offset) = 2^6 = 64 bytes

var wOffset = u.getParam("wOffset");


var is_illegal = 0;

if (wOffset < 6) is_illegal = 1;
if (wOffset > 51) is_illegal = 1;

if (is_illegal) {
    throw new Error("ERROR! ncore3_addr_mod6.tachl : It's illegal for wOffset=", wOffset);
}

//=============================================================================
// Ports
//=============================================================================

u.port('input',  'address', 52);
u.port('output', 'result',  3);

\jsend

module \=obj.lib.getModuleName()=\ (\=obj.lib.getPorts('\n')=\);

wire [2:0] in0;
wire [2:0] in1;
wire [2:0] in2;
wire [2:0] in3;
wire [2:0] in4;
wire [2:0] in5;
wire [2:0] in6;
wire [2:0] in7;
wire [2:0] in8;
wire [2:0] in9;
wire [2:0] in10;
wire [2:0] in11;

wire [2:0] sum00;
wire [2:0] sum01;
wire [2:0] sum02;
wire [2:0] sum03;
wire [2:0] sum04;
wire [2:0] sum05;

wire [2:0] sum10;
wire [2:0] sum11;
wire [2:0] sum12;

wire [2:0] sum20;
wire [2:0] sum21;

wire [2:0] out;

wire [45:0] operand;

wire [47:0] in;

\js if (wOffset == 6) {

assign operand = address[51:6];

\js } else if (wOffset == 51) {

assign operand = {45'b0, address[51:\=wOffset=\]};

\js } else {

assign operand = {\=46-(52-wOffset)=\'b0, address[51:\=wOffset=\]};

\js }

assign in = {2'b00, operand};

\=u.instance({instanceName: 'u0_lut0',  moduleName: 'ncore3_addr_mod6_lut0', ports: {in: 'in[3:0]',   out: 'in0'} })=\
\=u.instance({instanceName: 'u1_lut0',  moduleName: 'ncore3_addr_mod6_lut0', ports: {in: 'in[7:4]',   out: 'in1'} })=\
