\=u.instance({instanceName: 'u00_lut',  moduleName: 'ncore3_addr_mod6_lut1c', ports: {in0: 'in0',  in1: 'in1',  out: 'sum00'} })=\
\=u.instance({instanceName: 'u01_lut',  moduleName: 'ncore3_addr_mod6_lut1',  ports: {in0: 'in2',  in1: 'in3',  out: 'sum01'} })=\
