{
    "version": "0.1",
    "iter": "1",
    "plusargs": {
        "+UVM_MAX_QUIT_COUNT": "1",
        "+UVM_VERBOSITY": "UVM_NONE",
        "+UVM_TIMEOUT": "10000000000",
        "+max_interleave":"1",
        "+en_excl_txn":1,
        "+k_targ_src_id_chk_en": 1
    },
    "defines": "ASSERT_ON,COVER_ON,INHOUSE_APB_VIP,RESILIENCY_TESTING,INHOUSE_AXI",
    "ignoreJson": "$WORK_TOP/rtl/top/ignore.json",
    "tbTopTachl": "$WORK_TOP/rtl/giu/src/giu.tachl",
    "pkgPath"   : "giu/tb/dvPkg.json",
    "rtlTopCoverage": "tb_top/giu0.",
    "configlist": {
        "hw_d2d_config_socionext": {
            "purpose": "D2D Bringup",
            "tcl_lib": "hw_d2d_config_socionext_v0.5",
            "tcl_dir": "$MAESTRO_EXAMPLES/../base_configs/hw_d2d_config_socionext_v0.5",
	        "instance" : "giu0",
            "chiplet" : "DIE_1",
            "compile_args": "",
            "label":["weekly"]
        },
        "d2d_bringup": {
            "purpose": "D2D Bringup",
            "tcl_lib": "d2d_bringup",
            "tcl_dir": "$MAESTRO_EXAMPLES/../base_configs/d2d_bringup",
	        "instance" : "giu0",
            "compile_args": "",
            "label":["weekly"]
        },
        "hw_d2d_config_01": {
            "purpose": "D2D Bringup dual die",
            "tcl_lib": "hw_d2d_config_01",
            "tcl_dir": "$MAESTRO_EXAMPLES/../base_configs/hw_d2d_config_01",
	        "instance" : "giu0",
            "compile_args": "",
            "label":["weekly"]
        }
    },
    "testlist": {
        "random": {
            "name": "giu_bringup_test",
            "purpose": "Bringup test",
            "config": [
                "hw_d2d_config_socionext",
                "d2d_bringup",
                "hw_d2d_config_01"
            ],
            "iter": 5,
            "plusargs": {
                "+sysco_enable": "1",
                "+giu_sb_en": "1"
            },
            "label": [
                "regr",  "weekly",
				"regr_req",
                "mini_regr",
                "regr_cov"
            ]
        },
        "bringup_test": {
            "name": "giu_bringup_test",
            "purpose": "simple traffic",
            "config": [
                "hw_d2d_config_socionext",
                "d2d_bringup",
                "hw_d2d_config_01"
            ],
            "iter": 1,
            "plusargs": {
            },
            "label": [
			    "regr_req",
                "regr_cov",
                "regr",
                "weekly"
            ]
        },
        "csr_test": {
            "name": "giu_ral_test",
            "purpose": "access csrs",
            "config": [
                "hw_d2d_config_socionext",
                "d2d_bringup",
                "hw_d2d_config_01"
            ],
            "iter": 1,
            "plusargs": {
            },
            "label": [
			    "regr_req",
                "regr_cov",
                "regr",
                "weekly",
                "csr"
            ]
        },
        "unmapped_CSR_addr_access": {
            "name": "giu_ral_test",
            "purpose": "access unmapped csr address",
            "config": [
                "hw_d2d_config_socionext",
                "d2d_bringup",
                "hw_d2d_config_01"
            ],
            "iter": 1,
            "plusargs": {
                "+is_giu_dtwdbg_reader": "0",
                "+k_csr_seq":"giu_unmapped_csr_addrs_seq",
                "+k_num_requests": "50"
            },
            "label": [
			"regr_req",
                "regr_cov",
                "regr",
                "weekly",
                "csr"
            ]
        },
        "csr_hw_reset": {
            "name": "giu_ral_test",
            "purpose": "uvm built in hw reset values of all regs",
            "config": [
                "hw_d2d_config_socionext",
                "d2d_bringup",
                "hw_d2d_config_01"
            ],
            "iter": 1,
            "plusargs": {
		        "+is_giu_dtwdbg_reader": "0",
                "+k_csr_seq": "uvm_reg_hw_reset_seq"
            },
            "label": [
			    "regr_req",
                "regr",
                "weekly",
                "csr",
                "hw_reset"
            ]
        },
        "csr_bit_bash": {
            "name": "giu_ral_test",
            "purpose": "uvm built in bit bash seq for all regs",
            "config": [
                "hw_d2d_config_socionext",
                "d2d_bringup",
                "hw_d2d_config_01"
            ],
            "iter": 1,
            "plusargs": {
		        "+is_giu_dtwdbg_reader": "0",
                "+k_csr_seq": "uvm_reg_bit_bash_seq"
            },
            "label": [
			    "regr_req",
                "regr",
                "weekly",
                "regr_cov",
                "mini_regr",
                "csr",
                "bit_bash"
            ]
        }
    }
}
       
        
