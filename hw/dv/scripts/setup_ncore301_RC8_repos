cd $WORK_TOP
git config --unset-all remote.origin.fetch
git config --add remote.origin.fetch +refs/heads/Ncore3:refs/remotes/origin/Ncore3
git config --add remote.origin.fetch +refs/heads/master:refs/remotes/origin/master
git fetch
git checkout ncore3_rc8
echo "hw-ncr: Ncore3 using ncore3_rc8"

cd ../hw-ccp/
git config --unset-all remote.origin.fetch
git config --add remote.origin.fetch +refs/heads/Ncore3:refs/remotes/origin/Ncore3
git config --add remote.origin.fetch +refs/heads/master:refs/remotes/origin/master
git fetch
git checkout Ncore3
echo "hw-ccp: using Ncore3 Branch"

cd ../hw-lib/
git config --unset-all remote.origin.fetch
git config --add remote.origin.fetch +refs/heads/Ncore3:refs/remotes/origin/Ncore3
git config --add remote.origin.fetch +refs/heads/master:refs/remotes/origin/master
git fetch
git checkout Ncore3
echo "hw-lib: using Ncore3 Branch"

cd ../hw-sym/
git config --unset-all remote.origin.fetch
git config --add remote.origin.fetch +refs/heads/Ncore3:refs/remotes/origin/Ncore3
git config --add remote.origin.fetch +refs/heads/master:refs/remotes/origin/master
git fetch
git checkout Ncore3
echo "hw-sym: using Ncore3 Branch"

cd ../hw-scripts/
git config --unset-all remote.origin.fetch
git config --add remote.origin.fetch +refs/heads/Ncore3:refs/remotes/origin/Ncore3
git config --add remote.origin.fetch +refs/heads/master:refs/remotes/origin/master
git fetch
git checkout Ncore3
echo "hw-sym: using Ncore3 Branch"

cd ../hw-test-projects/
git config --unset-all remote.origin.fetch
git config --add remote.origin.fetch +refs/heads/Ncore3:refs/remotes/origin/Ncore3
git config --add remote.origin.fetch +refs/heads/master:refs/remotes/origin/master
