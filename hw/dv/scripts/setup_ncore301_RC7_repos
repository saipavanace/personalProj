git config --add remote.origin.fetch +refs/heads/Ncore3:refs/remotes/origin/Ncore3
git config --add remote.origin.fetch +refs/heads/master:refs/remotes/origin/master
git fetch
git checkout Ncore3
echo "hw-lib: using Ncore3 Branch"

cd ../hw-sym/
git config --unset-all remote.origin.fetch
git config --add remote.origin.fetch +refs/heads/Ncore3:refs/remotes/origin/Ncore3
git config --add remote.origin.fetch +refs/heads/master:refs/remotes/origin/master
git fetch
git checkout Ncore3
echo "hw-sym: Ncore3 Branch"

cd ../hw-scripts/
git config --unset-all remote.origin.fetch
git config --add remote.origin.fetch +refs/heads/Ncore3:refs/remotes/origin/Ncore3
git config --add remote.origin.fetch +refs/heads/master:refs/remotes/origin/master
git fetch
git checkout Ncore3
echo "hw-sym: using Ncore3 Branch"

cd ../hw-test-projects/
git config --unset-all remote.origin.fetch
git config --add remote.origin.fetch +refs/heads/Maestro-1.1.0-RC7-2021-04-29:refs/remotes/origin/Maestro-1.1.0-RC7-2021-04-29
git config --add remote.origin.fetch +refs/heads/Ncore3:refs/remotes/origin/Ncore3
git config --add remote.origin.fetch +refs/heads/master:refs/remotes/origin/master
