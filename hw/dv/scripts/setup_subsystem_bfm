cd $WORK_TOP
git config --unset-all remote.origin.fetch
git config --add remote.origin.fetch +refs/heads/Ncore3.7/New_SS_BFM:refs/remotes/origin/Ncore3.7/New_SS_BFM
git config --add remote.origin.fetch +refs/heads/Ncore3.7/develop:refs/remotes/origin/Ncore3.7/develop
git config --add remote.origin.fetch +refs/heads/master:refs/remotes/origin/master
git fetch
git checkout Ncore3.7/New_SS_BFM
echo "hw-ncr: Ncore3 using Ncore3.7/New_SS_BFM"
