{
  "name": "SmiPortRx",
  "version": "v0.1.0",
  "tachl": [
    {
      "name": "SmiPortRxParam",
      "params": [
        {
          "parameterName": "name",
          "type": "string",
          "parameterType": "architecture",
	  "noConstraints" : true
        },
        {
          "parameterName": "params",
          "type": "object",
          "reference": "SmiPortRxParamsParam",
          "parameterType": "architecture"
        }
      ]
    },
    {
      "name": "SmiPortRxParamsParam",
      "params": [
        {
          "parameterName": "fnMsgClass",
          "type": "array",
          "parameterType": "architecture",
          "arrayType": "string",
	  "minimum" : 1,
	  "maximum" : 16,
	  "default" : 1,
          "formula": "return []"
        },
        {
          "parameterName": "fPortId",
          "type": "array",
          "parameterType": "architecture",
	  "minimum" : 1,
	  "maximum" : 16,
	  "default" : 1,
	  "noEntConstraints" : true,	  
          "arrayType": "string",
          "formula": "return []"
        },
        {
          "parameterName": "fnMsgCmType",
          "type": "array",
          "parameterType": "architecture",
	  "minimum" : 1,
	  "maximum" : 16,
	  "default" : 1,
          "arrayType": "string"
        },
        {
          "parameterName": "enablePipe",
