          "parameterType": "architecture",
	  "minimum" : 0,
	  "maximum" : 1,
          "default": 0 
        },
        {
          "parameterName": "pipeDepth",
          "type": "integer",
          "parameterType": "architecture",
	  "minimum" : 0,
	  "maximum" : 2,
          "default": 2
        },
        {
