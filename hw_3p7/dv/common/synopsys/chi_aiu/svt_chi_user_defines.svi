 `define SVT_CHI_MIN_REQTOPCREDITGRANT_DELAY `SVT_CHI_LONG_DELAY_MIN_VAL

/** Minimum value for req_to_dbid_flit_delay */
 `define SVT_CHI_MIN_REQTODBID_DELAY `SVT_CHI_LONG_DELAY_MIN_VAL

`ifndef SVT_CHI_LONG_DELAY_MAX_VAL
