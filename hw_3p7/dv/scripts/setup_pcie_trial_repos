cd $WORK_TOP
git config --unset-all remote.origin.fetch
git config --add remote.origin.fetch +refs/heads/pcie_trial:refs/remotes/origin/pcie_trial
git config --add remote.origin.fetch +refs/heads/Ncore3:refs/remotes/origin/Ncore3
git config --add remote.origin.fetch +refs/heads/master:refs/remotes/origin/master
git fetch
git checkout pcie_trial
echo "hw-ncr: using ncore3_pcie_trial Branch"

cd ../hw-ccp/
git config --unset-all remote.origin.fetch
git config --add remote.origin.fetch +refs/heads/Ncore3:refs/remotes/origin/Ncore3
git config --add remote.origin.fetch +refs/heads/master:refs/remotes/origin/master
git fetch
git checkout Ncore3
echo "hw-ccp: using Ncore3"

cd ../hw-lib/
git config --unset-all remote.origin.fetch
git config --add remote.origin.fetch +refs/heads/Ncore3:refs/remotes/origin/Ncore3
git config --add remote.origin.fetch +refs/heads/master:refs/remotes/origin/master
git fetch
git checkout Ncore3
echo "hw-lib: using Ncore3"

cd ../hw-sym/
git config --unset-all remote.origin.fetch
git config --add remote.origin.fetch +refs/heads/Ncore3:refs/remotes/origin/Ncore3
git config --add remote.origin.fetch +refs/heads/master:refs/remotes/origin/master
git fetch
git checkout Ncore3
echo "hw-sym: using Ncore3"

cd ../hw-scripts/
git config --unset-all remote.origin.fetch
git config --add remote.origin.fetch +refs/heads/Ncore3:refs/remotes/origin/Ncore3
git config --add remote.origin.fetch +refs/heads/master:refs/remotes/origin/master
git fetch
git checkout Ncore3
echo "hw-sym: using Ncore3 Branch"

cd ../hw-test-projects/
git config --unset-all remote.origin.fetch
git config --add remote.origin.fetch +refs/heads/pcie_trial:refs/remotes/origin/pcie_trial
git config --add remote.origin.fetch +refs/heads/Ncore3:refs/remotes/origin/Ncore3
git config --add remote.origin.fetch +refs/heads/master:refs/remotes/origin/master
git fetch
git stash
git checkout pcie_trial
echo "hw-ncr: using ncore3_pcie_trial Branch"

cd ../hw-ncr/
