cd $WORK_TOP
git config --unset-all remote.origin.fetch
git config --add remote.origin.fetch +refs/heads/Ncore320_Beta_2:refs/remotes/origin/Ncore320_Beta_2
git config --add remote.origin.fetch +refs/heads/Ncore3:refs/remotes/origin/Ncore3
git config --add remote.origin.fetch +refs/heads/master:refs/remotes/origin/master
git fetch
git checkout Ncore320_Beta_2
echo "hw-ncr: Ncore3 using branch Ncore320_Beta_2"

cd ../hw-ccp/
git config --unset-all remote.origin.fetch
git config --add remote.origin.fetch +refs/heads/Ncore3:refs/remotes/origin/Ncore3
git config --add remote.origin.fetch +refs/heads/master:refs/remotes/origin/master
git fetch
git checkout ncore320_beta_2
echo "hw-ccp: using Ncore3 ncore320_beta_2"

cd ../hw-lib/
git config --unset-all remote.origin.fetch
git config --add remote.origin.fetch +refs/heads/Ncore3:refs/remotes/origin/Ncore3
git config --add remote.origin.fetch +refs/heads/master:refs/remotes/origin/master
git fetch
git checkout ncore320_beta_2
echo "hw-lib: using Ncore3 ncore320_beta_2"

cd ../hw-sym/
git config --unset-all remote.origin.fetch
git config --add remote.origin.fetch +refs/heads/Ncore3:refs/remotes/origin/Ncore3
git config --add remote.origin.fetch +refs/heads/master:refs/remotes/origin/master
git fetch
git checkout ncore320_beta_2
echo "hw-sym: using Ncore3 ncore320_beta_2"

cd ../hw-scripts/
git config --unset-all remote.origin.fetch
git config --add remote.origin.fetch +refs/heads/Ncore3:refs/remotes/origin/Ncore3
git config --add remote.origin.fetch +refs/heads/master:refs/remotes/origin/master
git fetch
git checkout ncore320_beta_2
echo "hw-scripts: using Ncore3 ncore320_beta_2"

cd ../hw-test-projects/
git config --unset-all remote.origin.fetch
git config --add remote.origin.fetch +refs/heads/Ncore3:refs/remotes/origin/Ncore3
git config --add remote.origin.fetch +refs/heads/master:refs/remotes/origin/master
git fetch
git checkout ncore320_beta_2
echo "hw-test-projects: using Ncore3 ncore320_beta_2"

cd ../hw-ncr/
